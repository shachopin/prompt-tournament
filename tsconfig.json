  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", ".next/types/**/*.ts", "db/schema.ts", "db/index.ts", "app/actions/auth.js", "app/actions/issues.js", "app/dashboard/page.jsx", "app/dashboard/layout.jsx", "app/dashboard/error.jsx", "app/issues/new/page.jsx", "app/components/UserEmail.jsx", "app/components/Navigation.jsx", "app/layout.jsx", "app/components/IssueForm.jsx", "app/components/NewIssue.jsx", "app/issues/[id]/page.jsx", "app/issues/[id]/loading.jsx", "app/issues/[id]/edit/page.jsx", "app/components/DashboardButton.jsx", "app/components/SignOutButton.jsx", "app/components/NavLink.jsx", "app/components/DashboardSkeleton.jsx", "app/components/DeleteIssueButton.jsx", "app/components/IssueCard.jsx", "app/providers.jsx", "app/(marketing)/layout.jsx", "app/(marketing)/page.jsx", "app/components/Timestamp.jsx", "lib/utils.js"],
